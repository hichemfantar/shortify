  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.17",
    "@types/http-errors": "^2",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "prisma": "^6.3.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "typescript": "5.7.3"
